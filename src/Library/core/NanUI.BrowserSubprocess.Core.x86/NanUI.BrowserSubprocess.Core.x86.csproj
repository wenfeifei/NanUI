    <Import Project="..\..\shared\BrowserSubprocess.SharedProject\BrowserSubprocess.SharedProject.projitems" Label="Shared" />

    <ItemGroup>
      <ProjectReference Include="..\NetDimension.NanUI.Core\NetDimension.NanUI.Core.csproj" />
    </ItemGroup>



